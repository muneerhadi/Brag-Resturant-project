{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-color",
          "scrollbar-width: thin"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 137",
    "not firefox <= 138",
    "not ios_saf <= 18.4",
    "not safari <= 18.4"
  ]
}